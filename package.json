{
  "name": "nextjs_template",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run lint && lint-staged -c .lintstagedrc.js && if [ -n \"$(git diff --cached --name-only --diff-filter=DR -- .env-sample)\"  ]; then echo 'Error : Do not rename or remove .env-sample, it should be copy to .env'; exit 1; fi",
    "prepare": "husky install && husky add .husky/pre-commit",
    "dump": "docker-compose exec db bash -c \"mysqldump -uroot -p\\$MYSQL_ROOT_PASSWORD -hlocalhost -B \\$MYSQL_DATABASE -v --skip-comments -r /docker-entrypoint-initdb.d/wp.sql && gzip -9vf /docker-entrypoint-initdb.d/wp.sql\"",
    "composer-setup": "docker-compose exec web bash -c \"cd /var/www/composer && php -r \\\"copy('https://getcomposer.org/installer', 'composer-setup.php');\\\" && php composer-setup.php\"",
    "composer-install": "docker-compose exec web bash -c \"cd /var/www/composer && php composer.phar install\"",
    "composer-require": "docker-compose exec web bash -c \"cd /var/www/composer && php composer.phar require\"",
    "block-install": "cd blocks && npm ci",
    "block-start": "cd blocks && npm start",
    "block-build": "cd blocks && npm run build",
    "setup": "bash scripts/setup.sh"
  },
  "dependencies": {
    "gsap": "^3.9.1",
    "next": "^12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "8.5.0",
    "husky": "^7.0.4",
    "image-trace-loader": "^1.0.2",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "9.0.0",
    "jimp": "^0.16.1",
    "lint-staged": "^12.4.0",
    "lqip-loader": "^2.2.1",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "postcss": "^8.4.12",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-nesting": "^10.1.3",
    "prettier": "2.6.2",
    "sass": "^1.43.4",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.0.15",
    "tailwindcss-fluid-spacing": "^0.4.0",
    "typescript": "^4.6.3",
    "webp-loader": "^0.6.0"
  },
  "engineStrict": true
}